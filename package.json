    },
    "devDependencies": {
        "@tauri-apps/cli": "^1",
        "@types/node": "^20.19.26",
        "@types/react": "^18.3.27",
        "@types/react-dom": "^18.3.7",
        "cross-env": "^7.0.3",
        "eslint": "^8",
        "eslint-config-next": "14.0.0",
        "typescript": "^5"
